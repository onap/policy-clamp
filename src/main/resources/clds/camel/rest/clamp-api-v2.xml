						uri="bean:org.onap.clamp.loop.log.LoopLogService?method=addLog('DELETE request successfully executed','INFO',${exchangeProperty[loopObject]})" />
					<to
						uri="bean:org.onap.clamp.flow.log.FlowLogOperation?method=endLog()" />
					<doCatch>
